{
  "extends": "lighthouse:default",
  "settings": {
    "formFactor": "desktop",
    "screenEmulation": {
      "mobile": false,
      "width": 1350,
      "height": 940,
      "deviceScaleFactor": 1,
      "disabled": false
    },
    "emulatedUserAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/98.0.4695.0 Safari/537.36",
    "waitForLoad": true
  },
  "audits": [
    "metrics/first-contentful-paint",
    "metrics/largest-contentful-paint",
    "metrics/total-blocking-time",
    "metrics/cumulative-layout-shift",
    "metrics/speed-index",
    "metrics/interactive",
    "metrics/first-meaningful-paint",
    "metrics/max-potential-fid",
    "server-response-time",
    "network-requests",
    "resource-summary",
    "third-party-summary",
    "byte-efficiency/uses-long-cache-ttl",
    "byte-efficiency/uses-optimized-images",
    "byte-efficiency/uses-responsive-images",
    "byte-efficiency/uses-text-compression",
    "byte-efficiency/modern-image-formats",
    "byte-efficiency/offscreen-images",
    "byte-efficiency/render-blocking-resources",
    "byte-efficiency/unminified-css",
    "byte-efficiency/unminified-javascript",
    "byte-efficiency/unused-css-rules",
    "byte-efficiency/unused-javascript",
    "dobetterweb/dom-size",
    "critical-request-chains",
    "user-timings",
    "bootup-time",
    "mainthread-work-breakdown",
    "font-display",
    "uses-rel-preconnect",
    "uses-rel-preload",
    "diagnostics",
    "largest-contentful-paint-element",
    "layout-shifts",
    "lcp-lazy-loaded",
    "long-tasks",
    "main-thread-tasks",
    "prioritize-lcp-image",
    "unsized-images",
    "work-during-interaction"
  ]
}